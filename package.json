{
  "private": true,
  "packageManager": "pnpm@7.3.0",
  "scripts": {
    "dev": "cd docs && npm run dev",
    "dev-pnpm": "pnpm run --filter library-demos dev",
    "dev-npm": "npm run dev --workspace library-demos",
    "build:docs-react": "pnpm i && cd docs && npm run build"
  },
  "engines": {
    "node": ">= 16"
  },
  "dependencies": {
    "@zsjs/hooks": "workspace:^",
    "@zsjs/mui-components": "workspace:^",
    "@zsjs/utils": "workspace:^",
    "@zsjs/ws-controllers": "workspace:^",
    "ts-deepmerge": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.4.1",
    "vite": "^5.0.10",
    "@types/node": "^18.15.11"
  },
  "//comment": "The following workspaces config is used by `npm workspaces`. You can delete it if you use pnpm.",
  "workspaces": [
    "packages/*",
    "docs"
  ]
}